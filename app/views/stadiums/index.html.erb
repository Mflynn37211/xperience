<div class="slideshow row" style="margin-top: 20px">

<div>
  <% @stadiums.each do |stadium| %>
    <div class="panel ">
      <%= image_tag stadium['photo'], style:"height: 200px; width: 400px;" %><br>
      <%= link_to stadium.name, stadium_path(stadium)%><br>
      <%=  stadium.city %>,<%=  stadium.state %><br>
      <%=  stadium.team %>
    </div>


<% end %>
</div>

<%= paginate @stadiums %>
